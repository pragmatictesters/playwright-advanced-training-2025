    "allure-playwright": "^3.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "prettier": "^3.4.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "orangehrm",
    "typescript",
    "e2e"
